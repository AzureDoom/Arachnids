	"authors": [
		"AzureDoom",
		"Batpixxler"
	],
	"contact": {
		"homepage": "https://www.curseforge.com/minecraft/mc-mods/arachnids",
		"sources": "https://github.com/AzureDoom/Arachnids",
		"issues": "https://github.com/AzureDoom/Arachnids/issues"
	},
	"license": "CC-BY-NC-4.0",
	"icon": "assets/arachnids/icon.png",
